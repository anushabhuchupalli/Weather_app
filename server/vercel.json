{
    "version": 2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/index.js" }
    ],
    "env": {
      "MONGODB_URI": "mongodb://127.0.0.1:27017/authentication",
      "JWT_SECRET": "123456789"
    }
  }
  